<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >
    <!-- cor do fundo -->
    <solid
        android:color="@color/corBotaoFundo"
        />
    <!-- cor e espessura da borda -->
    <stroke
        android:width="1dp"
        android:color="@color/corBotaoBorda"
        />
    <!-- propr radius arredonda os cantos -->
    <corners
        android:radius="12dp"
        />
    <padding
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        android:left="0dp"
        />
</shape>